o:graad a skos:Collection,
        skos:OrderedCollection ;
    rdfs:label "collectie onderwijsgraden"@nl ;
    skos:member ogra:lager-1e-graad,
        ogra:lager-2e-graad,
        ogra:lager-3e-graad,
        ogra:secundair-1e-graad,
        ogra:secundair-2e-graad,
        ogra:secundair-3e-graad ;
    skos:memberList ( ogra:lager-1e-graad ogra:lager-2e-graad ogra:lager-3e-graad ogra:secundair-1e-graad ogra:secundair-2e-graad ogra:secundair-3e-graad ) .

o:leerjaar a skos:Collection,
        skos:OrderedCollection ;
    rdfs:label "onderwijs leerjaren"@nl ;
    skos:member olee:kleuter-3-jaar-of-jonger,
        olee:kleuter-4-jaar,
        olee:kleuter-5-jaar,
        olee:lager-1e-leerjaar,
        olee:lager-2e-leerjaar,
        olee:lager-3e-leerjaar,
        olee:lager-4e-leerjaar,
        olee:lager-5e-leerjaar,
        olee:lager-6e-leerjaar,
        olee:secundair-1e-leerjaar,
        olee:secundair-2e-leerjaar,
        olee:secundair-3e-leerjaar,
        olee:secundair-4e-leerjaar,
        olee:secundair-5e-leerjaar,
        olee:secundair-6e-leerjaar,
        olee:secundair-7e-leerjaar ;
    skos:memberList ( olee:kleuter-3-jaar-of-jonger olee:kleuter-4-jaar olee:kleuter-5-jaar olee:lager-1e-leerjaar olee:lager-2e-leerjaar olee:lager-3e-leerjaar olee:lager-4e-leerjaar olee:lager-5e-leerjaar olee:lager-6e-leerjaar olee:secundair-1e-leerjaar olee:secundair-2e-leerjaar olee:secundair-3e-leerjaar olee:secundair-4e-leerjaar olee:secundair-5e-leerjaar olee:secundair-6e-leerjaar olee:secundair-7e-leerjaar ) .

o:niveau a skos:Collection ;
    rdfs:label "onderwijsniveaus"@nl ;
    skos:member oniv:basisonderwijs,
        oniv:deeltijds-kunstonderwijs,
        oniv:hoger-onderwijs,
        oniv:secundair-onderwijs,
        oniv:volwassenenonderwijs .

o:subniveau a skos:Collection ;
    rdfs:label "onderwijs subniveaus"@nl ;
    skos:member osub:basiseducatie,
        osub:kleuteronderwijs,
        osub:lager-onderwijs .

